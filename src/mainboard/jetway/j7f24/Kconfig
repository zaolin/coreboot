#	hex
#	default 0x8000
#	depends on BOARD_JETWAY_J7F24

config MAINBOARD_PART_NUMBER
	string
	default "j7f24"
	depends on BOARD_JETWAY_J7F24

config VIDEO_MB
	int
	default 32
	depends on BOARD_JETWAY_J7F24

config RAMBASE
	hex
	default 0x4000
	depends on BOARD_JETWAY_J7F24

config IRQ_SLOT_COUNT
	int
	default 10
	depends on BOARD_JETWAY_J7F24
