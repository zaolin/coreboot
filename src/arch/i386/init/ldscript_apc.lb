/* INPUT(coreboot_ap.rom)*/
INCLUDE "ldoptions"
SECTIONS
{
        .apcrom . : {
                _apcrom = .;
		*(.text)
		*(.rodata)
		*(.rodata.*)
                _eapcrom = .;
        }
        _iseg_apc = CONFIG_DCACHE_RAM_BASE;
        _eiseg_apc = _iseg_apc + SIZEOF(.apcrom);
        _liseg_apc = _apcrom;
        _eliseg_apc = _eapcrom;
}
