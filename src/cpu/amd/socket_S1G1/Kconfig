config CPU_AMD_SOCKET_S1G1
        bool
	default n
	select K8_REV_F_SUPPORT
	select K8_HT_FREQ_1G_SUPPORT
	select CPU_AMD_MODEL_FXX

config CPU_SOCKET_TYPE
	hex
	default 0x12
	depends on CPU_AMD_SOCKET_S1G1

#DDR2 and REG, S1G1
config DIMM_SUPPORT
	hex
	default 0x0204
	depends on CPU_AMD_SOCKET_S1G1

